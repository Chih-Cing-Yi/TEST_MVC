@if (TempData["Success"] != null)
    {
        <div id="alert" class="alert alert-success alert-dismissible fade in shadowed">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">×</span>
            </button>
            <strong>@TempData["Success"]</strong>
        </div>
        <script>
            let n = setInterval(()=>{
                document.querySelector('div#alert').hidden=true
                if(document.querySelector('div#alert').hidden == true){
                    clearInterval(n)
                }
            },2000)
        </script>
    }
@if (TempData["Error"] != null)
    {
        <div id="alert" class="alert alert-danger alert-dismissible fade in shadowed">
             <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">×</span>
            </button>
            <strong>@TempData["Error"]</strong>
        </div>
        <script>
            let n = setInterval(()=>{
                document.querySelector('div#alert').hidden=true
                if(document.querySelector('div#alert').hidden == true){
                    clearInterval(n)
                }
            },2000)
        </script>
    }